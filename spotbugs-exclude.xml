<FindBugsFilter>
  <Match>
    <Or>
      <!-- TODO: Remove once we migrate to SpotBugs with full Java support (JENKINS-53720)-->
      <!-- This check works unreliably on JDK 11 which tries doing some code generation for annotated fields-->
      <Bug pattern="RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE"/>
    </Or>
  </Match>
  <Match>
      <!-- Ignore false positive NPE check which occurs in EC2AbstractSlave due to Spotbugs not quite -->
      <!-- understanding how Java serialization works. -->
      <Bug pattern="RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE"/>
  </Match>
</FindBugsFilter>
